<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="org.digitalcampus.mquiz"
      android:versionCode="4"
      android:versionName="1.1.2">
    <uses-sdk android:minSdkVersion="8" />

    <application android:icon="@drawable/dc_logo" android:label="@string/app_name">
        <activity android:name=".AssessmentActivity"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
		<activity android:name=".QuizActivity" android:label="Quiz"></activity>
		<activity android:name=".QuizActivityEnd" android:label="@string/quiz_summary"></activity>
		<activity android:name=".ResultsActivity" android:label="@string/quiz_attempts"></activity>
		<activity android:name=".SubmitActivity" android:label="@string/submit_activity_label"></activity>
		<activity android:name=".PrefsActivity" android:label="@string/title_prefs" />
		<activity android:name=".AboutActivity" android:label="@string/title_about" />
		<activity android:name=".SelectQuizActivity" android:label="@string/title_select_quiz" />
		<activity android:name=".ManageQuizActivity" android:label="@string/title_manage_quiz" />
		<activity android:name=".RegisterActivity" android:label="@string/title_register" />
    </application>
    
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
</manifest>